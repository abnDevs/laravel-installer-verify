{
    "name": "bishwajitcadhikary/laravel-installer-verify",
    "description": "Laravel Installer is a Laravel package.",
    "keywords": [
        "bishwajitcadhikary",
        "laravel",
        "installer"
    ],
    "homepage": "https://github.com/abnDevs",
    "license": "MIT",
    "authors": [
        {
            "name": "Bishwajit Adhikary",
            "email": "bishwajitcadhikary@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "jackiedo/dotenv-editor": "^2.1",
        "php-flasher/flasher-laravel": "^1.13",
        "spatie/laravel-markdown": "^2.3",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "autoload": {
        "psr-4": {
            "AbnDevs\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AbnDevs\\Installer\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbnDevs\\Installer\\InstallerServiceProvider",
                "Jackiedo\\DotenvEditor\\DotenvEditorServiceProvider"
            ],
            "aliases": {
                "Installer": "AbnDevs\\Installer\\Facades\\Installer",
                "License": "AbnDevs\\Installer\\Facades\\License"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
